{
  "name": "pricing",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "dodo:newpair:newaccount": "ts-node src/dodo/dodo_client.ts newacc",
    "dodo:newpair:deploytoken": "ts-node src/dodo/dodo_client.ts depoly",
    "dodo:newpair:minttoken": "ts-node src/dodo/dodo_client.ts m",
    "dodo:newpair:newdodo": "ts-node src/dodo/dodo_client.ts newdo",
    "dodo:newpair:setprice": "ts-node src/dodo/dodo_client.ts sp",
    "dodo:newpair:setparameter": "ts-node src/dodo/dodo_client.ts spa",
    "dodo:pricing:dev": "ts-node  -r dotenv/config src/dodo/pricing/PricingFormula.ts dotenv_config_path=.env.dev",
    "dodo:pricing:test": "ts-node  -r dotenv/config src/dodo/pricing/PricingFormula.ts dotenv_config_path=.env.test",
    "dodo:pricing:prod": "ts-node  -r dotenv/config src/dodo/pricing/PricingFormula.ts dotenv_config_path=.env.prod",
    "start": "ts-node src/picture.ts",
    "dev": "nodemon",
    "jest": "jest src/tests/*eosjs*",
    "test-node": "jest src/tests/*node*",
    "test": ""
  },
  "author": "",
  "license": "",
  "dependencies": {
    "decimal.js": "^10.2.1",
    "dotenv": "^6.0.0",
    "fs": "^0.0.1-security",
    "prettier": "^2.2.0",
    "roxejs": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/tests/setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
